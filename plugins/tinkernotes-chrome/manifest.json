{
  "name": "TinkerNotes extension",
  "version": "1.0",
  "manifest_version": 2,
  "description": "An extension for importing bookmarks into TinkerNotes",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": { "scripts": ["background.js"]},
  "permissions": [
    "history",
    "tabs",
    "http://localhost"
  ]
}
